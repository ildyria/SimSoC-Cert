\newcommand{\etelimg}[1]{%
  \RightLabel{$_{\etm\elimg}$}\UnaryInfC{#1}}
\newcommand{\etelimd}[1]{%
  \RightLabel{$_{\etm\elimd}$}\UnaryInfC{#1}}

\newcommand{\qqsintro}[1]{%
  \RightLabel{$_{\forall_\intro}$}\UnaryInfC{#1}}
\newcommand{\qqselim}[3]{%
  \RightLabel{$_{\forall_\elim{(\subs{#2}{#3})}}$}\UnaryInfC{#1}}

\newcommand{\exintro}[1]{%
  \RightLabel{$_{\exists_\intro}$}\UnaryInfC{#1}}
\newcommand{\exelim}[2]{%
  \RightLabel{$_{\exists_\elim\rouge{[#2]}}$}\BinaryInfC{#1}}

\newcommand{\eqintro}[1]{%
  \AxiomC{}%
  \RightLabel{$_{=\intro}$}\UnaryInfC{#1=#1}}
\newcommand{\eqelim}[1]{%
  \RightLabel{$_{=\elim}$}\BinaryInfC{#1}}

\newcommand{\botelim}[1]{%
  \RightLabel{$_{\bot\elim}$}\UnaryInfC{#1}}

\newcommand{\nomnnelim}{$\neg\neg\elim$}
\newcommand{\nnelim}[1]{%
  \RightLabel{$_{\neg\neg\elim}$}\UnaryInfC{#1}}

\newcommand{\regledef}[2]{%
  \dottedLine
  \RightLabel{$_{#1\,\DEF}$}\UnaryInfC{#2}}
\newcommand{\reglerac}[2]{%
  \doubleLine
  \RightLabel{$_{#1\,\DEF}$}\UnaryInfC{#2}}
\newcommand{\regledeflie}[3]{%
  \dottedLine
  \RightLabel{$_{#1\,\DEF}\rouge{[#3]}$}\UnaryInfC{#2}}
\newcommand{\regleraclie}[3]{%
  \doubleLine
  \RightLabel{$_{#1\,\DEF}\rouge{[#3]}$}\UnaryInfC{#2}}


\newcommand{\negdef}[1]{\regledef{\neg}{#1}}
\newcommand{\equivdef}[1]{\regledef{\equim}{#1}}
\newcommand{\uniondef}[1]{\regledef{\union}{#1}}
\newcommand{\interdef}[1]{\regledef{\inter}{#1}}
\newcommand{\incldef}[1]{\regledef{\inclus}{#1}}
\newcommand{\videdef}[1]{\regledef{\emptyset}{#1}}
\newcommand{\compldef}[1]{\regledef{\backslash}{#1}}
\newcommand{\partdef}[1]{\regledef{\mathcal{P}}{#1}}

%\newcommand{\inclrac}[2]{\regleraclie{\inclus}{#1}{#2}}
\newcommand{\inclrac}[2]{%
  \doubleLine
  \RightLabel{$_{\inclus\intro}\rouge{[#2]}$}\UnaryInfC{#1}}

\newcommand{\regleext}[1]{%
  \RightLabel{$_{\mathrm{ext}}$}\UnaryInfC{#1}}
\newcommand{\racext}[3]{%

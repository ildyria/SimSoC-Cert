%\newcommand{\DEF}{\ensuremath{\mathrm{d\acute{e}f}}}
\newcommand{\DEF}{\ensuremath{\textrm{def}}}

\newcommand{\implintronaif}[1]{%
  \RightLabel{$_{\implm\intro}$}\UnaryInfC{#1}}
\newcommand{\implintro}[2]{%
  \RightLabel{$_{\implm\intro\rouge{[#2]}}$}\UnaryInfC{#1}}
\newcommand{\implelim}[1]{%
  \RightLabel{$_{\implm\elim}$}\BinaryInfC{#1}}

\newcommand{\ouintrog}[1]{%
  \RightLabel{$_{\oum\introg}$}\UnaryInfC{#1}}
\newcommand{\ouintrod}[1]{%
  \RightLabel{$_{\oum\introd}$}\UnaryInfC{#1}}
\newcommand{\ouelim}[3]{%
